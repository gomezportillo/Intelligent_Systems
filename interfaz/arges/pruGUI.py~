from threading import Thread, Timer
import sys
import os
import shutil
import webbrowser
import datetime
import time

try:
    import gtk
except:
    print("GTK Not Available")
    sys.exit(1)


class ArgesGUI:
    builder = None
    window = None

    def __init__(self):
        gtk.gdk.threads_init()
        gladefile = "GUIArges.glade"
        self.builder = gtk.Builder()
        self.builder.add_from_file(gladefile)
        self.completo = False 
        
        handlers = {
            "on_BtnProcesar1_clicked": self.method
        }
