all: test

build:
	chmod 755 ./src/main.py
	mkdir -p ./data
	wget -O ./data/map.osm http://api.openstreetmap.org/api/0.6/map?bbox=-3.9719000,38.9650000,-3.8847000,39.0062000	

test:
	python src/main.py 3753271186 -3.9719000 38.9650000 -3.8847000 39.0062000 3753271182 818781159

test2:
	python src/main.py 3753271186 -3.9719000 38.9650000 -3.8847000 39.0062000 3753271182 3753271185

test3:
	python src/main.py 3753271186 -3.9719000 38.9650000 -3.8847000 39.0062000 3753271182 818781159

test4:
	python src/main.py 836098076 -3.9719000 38.9650000 -3.8847000 39.0062000 2711270358 2814223095

test5:
	python src/main.py 812954451 -3.9719000 38.9650000 -3.8847000 39.0062000 812954647 525959919 812954639

test6:
	python src/main.py 796725834 -3.9719000 38.9650000 -3.8847000 39.0062000 797147544 797147524 797147551

test_error:
	python src/main.py 3753271186 -3.9719000 38.9650000 -3.8847000 39.0062000 -1

clean:
	@rm -f src/*.pyc src/*~ *~ 
	@echo "Binary files deleted"

remove_all: clean
	@rm -fr data/   	
	@echo "OSM file deleted"


